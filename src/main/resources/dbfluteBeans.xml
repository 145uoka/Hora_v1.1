<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
    >

    <!-- The components of DBFlute Runtime. -->
    <bean id="introduction" class="com.olympus.hora.common.dbflute.ExtendedDBFluteInitializer">
        <constructor-arg index="0"><ref bean="dataSource"/></constructor-arg>
    </bean>
    <bean id="invokerAssistant" class="com.olympus.hora.dbflute.allcommon.ImplementedInvokerAssistant" autowire="byType">
        <property name="dataSource"><ref bean="dataSource"/></property>
        <property name="introduction"><ref bean="introduction"/></property>
    </bean>
    <bean id="commonColumnAutoSetupper" class="com.olympus.hora.dbflute.allcommon.ImplementedCommonColumnAutoSetupper" autowire="byType"/>
    <bean id="behaviorSelector" class="com.olympus.hora.dbflute.allcommon.ImplementedBehaviorSelector" autowire="byType"/>
    <bean id="behaviorCommandInvoker" class="org.dbflute.bhv.core.BehaviorCommandInvoker" autowire="byType">
        <property name="invokerAssistant"><ref bean="invokerAssistant"/></property>
    </bean>

    <!-- The components of Behavior. -->
	<bean id="mCodeBhv" class="com.olympus.hora.dbflute.exbhv.MCodeBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mCompanyBhv" class="com.olympus.hora.dbflute.exbhv.MCompanyBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mCourseBhv" class="com.olympus.hora.dbflute.exbhv.MCourseBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mCourseGroupBhv" class="com.olympus.hora.dbflute.exbhv.MCourseGroupBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mHolidayBhv" class="com.olympus.hora.dbflute.exbhv.MHolidayBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mShopBhv" class="com.olympus.hora.dbflute.exbhv.MShopBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mStaffBhv" class="com.olympus.hora.dbflute.exbhv.MStaffBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mUserBhv" class="com.olympus.hora.dbflute.exbhv.MUserBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mWorkingDayBhv" class="com.olympus.hora.dbflute.exbhv.MWorkingDayBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mWorkingDayDeffBhv" class="com.olympus.hora.dbflute.exbhv.MWorkingDayDeffBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mWorkingDayDetailDeffBhv" class="com.olympus.hora.dbflute.exbhv.MWorkingDayDetailDeffBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="mWorkingStaffBhv" class="com.olympus.hora.dbflute.exbhv.MWorkingStaffBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="tReservationBhv" class="com.olympus.hora.dbflute.exbhv.TReservationBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="tReservationDetailBhv" class="com.olympus.hora.dbflute.exbhv.TReservationDetailBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
	<bean id="tShiftBhv" class="com.olympus.hora.dbflute.exbhv.TShiftBhv" autowire="byType">
        <property name="behaviorCommandInvoker"><ref bean="behaviorCommandInvoker"/></property>
        <property name="behaviorSelector"><ref bean="behaviorSelector"/></property>
        <property name="commonColumnAutoSetupper"><ref bean="commonColumnAutoSetupper"/></property>
    </bean>
</beans>
